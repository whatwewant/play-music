{
  "entry": {
    "index": "./src/index.js",
    "vendor": [
      "react",
      "react-dom",
      "react-jss",
      "react-loadable",
      "reselect",
      "store",
      "qrcode.react",
      "dva",
      "antd-mobile",
      "fingerprintjs"
    ]
  },
  "multipage": true,
  "publicPath": "./",
  "extraBabelPlugins": [
    "transform-decorators-legacy",
    ["import", { "style": "css", "libraryName": "antd-mobile" }],
    "syntax-dynamic-import"
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
